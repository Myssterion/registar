<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Registar" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/teal_500</item>
        <item name="colorPrimaryVariant">@color/teal_800</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/deepOrange_500</item>
        <item name="colorSecondaryVariant">@color/deepOrange_900</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Registar.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Registar.Filter" parent="Theme.MaterialComponents.Light">
        <item name="colorPrimary">@color/teal_300</item>
        <item name="colorSecondary">@color/deepOrange_300</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorOnSecondary">@color/white</item>
    </style>

    <style name="Theme.Registar.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Registar.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>